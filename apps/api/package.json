{
  "name": "@aui/api",
  "private": false,
  "version": "1.0.0",
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "pnpm": "10.13.1"
  },
  "type": "module",
  "imports": {
    "#app/*": "./src/*",
    "#app/prisma/client/*": "./prisma/generated/prisma/client/*",
    "#app/prisma/seed/*": "./prisma/seed/*"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.ts"
    }
  },
  "scripts": {
    "db:clean": "rm -rf prisma/dev.db prisma/generated",
    "db:generate": "pnpm prisma generate",
    "db:seed": "node --loader ts-node/esm prisma/seed/seed.ts --tsconfig=tsconfig.json",
    "db": "pnpm run db:clean && pnpm run db:generate && pnpx prisma db push && pnpm run db:seed",
    "build": "",
    "dev": "node --loader ts-node/esm --watch --env-file=.env --experimental-default-config-file --no-warnings=ExperimentalWarning src/server.ts"
  },
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@koa/router": "^14.0.0",
    "@prisma/client": "^6.14.0",
    "jsonwebtoken": "^9.0.2",
    "koa": "^3.0.1",
    "koa-body": "^6.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/koa": "^3.0.0",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^24.3.0",
    "prisma": "^6.14.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
