  "version": "1.0.0",
  "packageManager": "pnpm@10.13.1",
  "type": "module",
  "scripts": {
    "setup": "rimraf -g **/node_modules pnpm-lock.yaml && pnpm install"
  },
  "dependencies": {
  },
  "devDependencies": {
    "rimraf": "^6.0.1"
  },
  "workspaces": [
    "apps/*"
  ],
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "@angular/core": "21.0.0-next.2",
        "@angular/common": "21.0.0-next.2",
        "@angular/ssr": "21.0.0-next.2"
      }
    }
  }
}
